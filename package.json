{
  "name": "electron-express-app",
  "version": "1.0.0",
  "description": "A Electron app with integration of express",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-express-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/windows/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Express Boilerplate\"",
    "package-linux": "electron-packager . electron-express-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/bricxcarasco/electron-express-app.git",
  "keywords": [
    "Electron",
    "express",
    "mysql",
    "app",
    "boiler plate"
  ],
  "author": "Bricx Carasco",
  "license": "BC-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1"
  }
}
